<div class="section" id="sec3">

    <div class="SecWrapper">
        <h1>Kontakt</h1>
        <hr class="title-hr" />
        <div class="row SecCon">
            <div class="col-md-3"></div>
            <div class="col-md-6">
                @using (Html.BeginForm("Contact", "Home", FormMethod.Post, new { @class = "form-horizontal" }))
                {

                    <div class="form-group">

                        <label for="navn">Navn:</label><input name="Navn" type="text" id="navn" class="form-control" placeholder="Skriv dit navn her" required />
                    </div>
                    <div class="form-group">
                        <label for="mail">Email:</label><input class="form-control" name="Email" id="mail" type="email" placeholder="Skriv din Email her" required />
                    </div>
                    <div class="form-group">
                        <label for="emne">Emne:</label><select id="emne" class="form-control" placeholder="Vælg hvad din henvendelse drejer sig om" name="Emne" required>
                            <option value="Reparation">IT reparationer</option>
                            <option value="KontorServer">Kontor & Server opsætning</option>
                            <option value="Web">Hjemmeside & Web</option>
                            <option value="Konsulent">Rådgivning & Konsuletation</option>
                            <option value="Andet">Andet</option>
                        </select>
                    </div>
                        <div class="form-group">
                            <label for="besked">Besked:</label>
                            <textarea class="form-control" id="besked" name="Message" required rows="13" cols="20"></textarea>
                        </div>
                            <button class="btn btn-default pull-right" type="submit">Send</button>

                }

            </div>
            <div class="col-md-3"></div>
        </div>
    </div>
</div>


